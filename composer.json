{
    "name": "duyler/config",
    "type": "library",
    "description": "Duyler config file loader",
    "license": "MIT",
    "keywords": [
        "config"
    ],
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "support": {
        "docs": "https://duyler.org/docs/config/"
    },
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.30"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Duyler\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duyler\\Config\\Test\\": ["tests/"]
        }
    }
}

